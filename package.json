    "@testing-library/user-event": "^7.2.1",
    "@types/react-dom": "^16.9.6",
    "axios": "latest",
    "classnames": "latest",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "latest",
    "react-scripts": "^3.4.1",
    "typescript": "^3.7.5",
    "update": "^0.7.4"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^4.1.3",
    "@storybook/addon-actions": "latest",
    "@storybook/addon-centered": "latest",
    "@storybook/addon-docs": "latest",
    "@storybook/addon-links": "latest",
    "@storybook/addons": "latest",
    "@storybook/cli": "latest",
    "@storybook/preset-create-react-app": "latest",
    "@storybook/react": "latest",
    "@types/enzyme": "^3.10.5",
    "@types/enzyme-adapter-react-16": "^1.0.6",
    "@types/jest": "25.1.4",
    "@types/node": "^12.12.34",
    "@types/react": "^16.9.32",
    "@types/react-router": "latest",
    "@types/react-router-dom": "latest",
    "awesome-typescript-loader": "latest",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "cypress": "^4.3.0",
    "enzyme": "^3.11.0",
    "enzyme-adapter-react-16": "^1.15.2",
    "enzyme-to-json": "^3.4.4",
    "eslint": "latest",
    "eslint-plugin-react-app": "^6.2.2",
    "husky": "latest",
    "jest": "^24.9.0",
    "lint-staged": "latest",
    "prettier": "latest",
    "react-docgen-typescript-loader": "latest",
    "ts-jest": "^25.3.0",
    "ts-loader": "^6.2.1",
    "webpack": "4.42.0"
  },
  "types": "index.d.ts",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/*.ts src/**/*.tsx",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "fix": "prettier --write src/**/*.ts src/**/*.tsx && eslint --fix src/**/*.ts src/**/*.tsx",
    "cp:open": "cypress open",
    "fmt:check": "prettier --check src/**/*.{js,ts,tsx}",
    "coverage:jest": "jest --coverage",
    "cp:run": "cypress run"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
