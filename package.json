{
  "name": "myapp",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@jetkit/react": "latest",
    "@material-ui/core": "latest",
    "@material-ui/icons": "latest",
    "@material-ui/styles": "latest",
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/react-dom": "^16.9.6",
    "axios": "latest",
    "classnames": "latest",
    "jetbridge-react-scripts": "1.3.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-router-dom": "latest",
    "typescript": "^3.7.5"
  },
  "devDependencies": {
    "@storybook/addon-actions": "latest",
    "@storybook/addon-centered": "latest",
    "@storybook/addon-docs": "latest",
    "@storybook/addon-links": "latest",
    "@storybook/addons": "latest",
    "@storybook/cli": "latest",
    "@storybook/preset-create-react-app": "latest",
    "@storybook/react": "latest",
    "@types/node": "^12.12.34",
    "@types/react": "^16.9.32",
    "@types/react-router": "latest",
    "@types/react-router-dom": "latest",
    "awesome-typescript-loader": "latest",
    "babel-eslint": "10.0.3",
    "babel-loader": "8.0.6",
    "eslint": "latest",
    "husky": "latest",
    "lint-staged": "latest",
    "prettier": "latest",
    "react-docgen-typescript-loader": "latest"
  },
  "types": "index.d.ts",
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "prettier --write",
      "eslint --fix",
      "git add"
    ]
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint src/**/*.ts src/**/*.tsx",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public",
    "fix": "prettier --write src/**/*.ts src/**/*.tsx && eslint --fix src/**/*.ts src/**/*.tsx"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
